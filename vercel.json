{
    "functions": {
      "server.js": {
        "runtime": "nodejs16.x",
        "memory": 1024,
        "maxDuration": 60
      }
    },
    "regions": ["iad1"],
    "routes": [
      {
        "src": "/socket.io/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }